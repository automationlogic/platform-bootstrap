# Used in terraform
TF_VAR_org_id=$(gcloud organizations list | tail -1 | awk '{ print $2 }')
GOOGLE_PROJECT="${INCEPTION_PROJECT}"
TF_VAR_orchestration_project_number=$(gcloud projects list | awk -v p="${INCEPTION_PROJECT}" '($1 == p) { print $3 }')
TF_VAR_analytics_project="climate-analytics"
TF_VAR_region="${REGION}"
TF_VAR_billing_account="${BILLING_ACCOUNT}"
TF_VAR_owner="<github repos account eg. thundercomb>"
TF_VAR_owner_email="<owner email>"
TF_VAR_kubeflow_host="<kubeflow host>"
TF_VAR_inception_ip=$(curl ifconfig.me)

# Note: kubeflow host will only become available after kubeflow has been installed
#       come back here, add the information, and rerun:
#       platform-inception, analytics-infra, and then training pipelines

export TF_VAR_org_id GOOGLE_PROJECT TF_VAR_orchestration_project_id TF_VAR_ORCHESTRATION_PROJECT_number TF_VAR_analytics_project TF_VAR_region TF_VAR_billing_account TF_VAR_owner TF_VAR_owner_email TF_VAR_kubeflow_host TF_VAR_inception_ip
